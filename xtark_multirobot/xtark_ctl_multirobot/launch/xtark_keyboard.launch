<launch>
    <param name="use_sim_time" value="false"/>
    <arg name="namespace" default="robot0"/>
    <!-- 启动键盘遥控节点 -->
    <group ns="$(arg namespace)">
    	<node name="xtark_twist_keyboard" pkg="xtark_ctl_multirobot" type="xtark_twist_keyboard.py" output="screen" respawn="false"/>
    </group>
</launch>
